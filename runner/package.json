{
  "name": "power-agent-benchmark-runner",
  "version": "1.0.0",
  "description": "Benchmark runner for Power Agent evaluation",
  "type": "module",
  "main": "run-benchmark.js",
  "scripts": {
    "benchmark": "node run-benchmark.js",
    "benchmark:tier1": "node run-benchmark.js --tier=1",
    "benchmark:tier2": "node run-benchmark.js --tier=2",
    "benchmark:tier3": "node run-benchmark.js --tier=3",
    "benchmark:tier4": "node run-benchmark.js --tier=4",
    "benchmark:all": "node run-benchmark.js --tier=1 && node run-benchmark.js --tier=2 && node run-benchmark.js --tier=3 && node run-benchmark.js --tier=4"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.52.0",
    "dotenv": "^16.3.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "benchmark",
    "power-analysis",
    "biostatistics",
    "llm-evaluation"
  ],
  "author": "Yukang Zeng",
  "license": "MIT"
}
